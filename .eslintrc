{
  "extends": "kellyirc",
  "cache": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "ace": true
  },
  "rules": {
    "strict": 0,
    "no-var": 2,
    "prefer-const": 2,
    "no-cond-assign": ["error", "except-parens"],
    "no-extra-parens": 0,
    "no-sparse-arrays": 0,
    "indent": ["error", 2],
    "no-undef": 2,
    "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }]
  }
}
